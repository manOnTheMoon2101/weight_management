generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Food {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  name      String?
  calories  Float?
  sugar     Float?
  protein   Float?
  iron      Float?
  carbs     Float?
  createdAt DateTime?
  updatedAt DateTime?
}

model Data {
  id                   String   @id @default(auto()) @map("_id") @db.ObjectId
  weight               Float
  createdAt            DateTime @default(now())
  updatedAt            DateTime
  workedOut            Boolean
  tookFatburner        Boolean
  tookWeightmanagement Boolean
  tookVitamin          Boolean
  totalCalories        Float?
  totalProtein         Float
  totalFat             Float
  totalCarbs           Float
  workoutTime          String
  totalSugar           Float
  averageWaterinML     Float
  foodsInt             Int[]
}
